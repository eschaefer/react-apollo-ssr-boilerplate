    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack-node-externals": "^1.6.0",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "scripts": {
    "build:ssr:dev": "NODE_ENV=development webpack --config ./webpack/webpack.config.dev-ssr.js",
    "build:ssr:prod": "BABEL_ENV=server NODE_ENV=production webpack --config ./webpack/webpack.config.prod-ssr.js",
    "build:client:prod": "NODE_ENV=production webpack --config ./webpack/webpack.config.prod-client.js",
    "build:server:prod": "NODE_ENV=production webpack --config ./webpack/webpack.config.prod-server.js",
    "build:all": "npm run clear:assets && npm run build:client:prod && npm run build:ssr:prod && npm run build:server:prod",
    "build:dev": "npm run build:ssr:dev && nodemon server/index.js --exec babel-node",
    "clear:assets": "rm -rf dist/assets/css/* && rm -rf dist/assets/js/*",
    "start:dev": "run-p build:dev test:watch",
    "start:prod": "node dist/server.js",
    "test": "jest -u --coverage",
    "test:watch": "jest -u --watchAll",
    "lint:js": "eslint app/**/*.{js,jsx}",
    "lint:styles": "sass-lint --config .sass-lint.yml '**/*.scss' --verbose --no-exit",
    "precommit": "lint-staged",
    "prepush": "npm run test"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.scss": "sass-lint --config .sass-lint.yml --verbose --no-exit"
  },
  "jest": {
    "collectCoverageFrom": [
      "(app|server|)/**/*.{js,jsx}"
    ],
    "coverageDirectory": ".",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "browserslist": [
    "defaults",
    "not ie <= 8"
  ]
}
